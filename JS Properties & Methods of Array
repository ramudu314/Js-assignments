function arrayProperties(arr1, arr2) {
//    write your code here
let arr3 = arr2.concat(arr1);

arr3.push(arr2);
console.log(arr3);

arr3.pop();
arr3.reverse();
console.log(arr3);

const lastIndexOrange = arr3.lastIndexOf("orange");
console.log(lastIndexOrange);

const sliceArray = arr3.slice(0,3);
console.log(sliceArray.length);

const joinedString = sliceArray.join(",");
console.log(joinedString);


}
